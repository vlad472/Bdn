	  var footerData = {};
	  var isDataProcessed = false;
	  var h = require$$0.h,
	      render = require$$0.render; // curried function for api functions that get component elements.

	  function getComponentData(componentKey) {
	    return function () {
	      if (!isDataProcessed) {
	        throw new Error('Footer data must be processed before calling api. Use .init() function.');
	      }

	      if (componentKey === 'all') {
	        return footerData;
	      }

	      if (!footerData[componentKey]) {
	        return null;
	      }

	      return footerData[componentKey];
	    };
	  }

	  function getComponentElement(styles, Component) {
	    return function (options) {
	      if (!isDataProcessed) {
	        throw new Error('Footer data must be processed before calling api. Use .init() function.');
	      }

	      if (!options || options && !options.disableStyles) {
	        common.appendStyles(styles);
	      }

	      var temp = document.createElement("div");
	      render(h(Component, footerData), temp);
	      return temp.firstChild;
	    };
	  }

	  var footerContent = module.exports = {
	    config: {},
	    styles: require$$1,
	    footerContainerElement: null,
	    // api data funcitons
	    getFooterData: getComponentData('all'),
	    getRiotLogoData: getComponentData('riotgames'),
	    getCopyrightData: getComponentData('copyright'),
	    getFooterLinksData: getComponentData('footerLinks'),
	    getSocialLinksData: getComponentData('socialLinks'),
	    getLegalLinksData: getComponentData('legalLinks'),
	    getGameRatingLogosData: getComponentData('gameRatings'),
	    // api render functions
	    getFooterLinksElement: getComponentElement(stylesFooterLinks, FooterLinks),
	    getRiotLogoElement: getComponentElement(stylesRiotLogo, RiotLogo),
	    getCopyrightElement: getComponentElement(stylesCopyright, Copyright),
	    getLegalLinksElement: getComponentElement(stylesLegalLinks, LegalLinks),
	    getSocialLinksElement: getComponentElement(stylesSocialLinks, SocialLinks),
	    getGameRatingLogosElement: getComponentElement(stylesGameRatingLogos, GameRatings),
	    determineContainerElement: function determineContainerElement() {
	      var config = this.config;
	      return new Promise(function (resolve, reject) {
	        if (config && config.container && config.container.renderFooterInto) {
	          var footerContainerElement = document.querySelector(config.container.renderFooterInto);

	          if (footerContainerElement) {
	            resolve(footerContainerElement);
	          } // return false if renderFooterInto element didn't exist.


	          reject();
	        } else if (RiotBar.data && RiotBar.data.footer && RiotBar.data.footer.renderInto) {
	          // A timer to keep checking for the element
	          var checkForEl = function checkForEl() {
	            var el = document.getElementById(RiotBar.data.footer.renderInto);

	            if (el) {
	              resolve(el);
	            } else {
	              setTimeout(checkForEl, 50);
	            }
	          };

	          checkForEl();
	        } else {
	          // append to body by default.
	          var container = document.createElement('div');
	          document.body.appendChild(container);
	          resolve(container);
	        }
	      });
	    },

	    /**
	     * Process Data before rendering.
	     */
	    processData: function processData() {
	      this.processLegalLinkClasses();
	      this.processCopyrightYear();
	      this.processCopyrightMessageLinks();
	      isDataProcessed = true;
	    },
	    processLegalLinkClasses: function processLegalLinkClasses() {
	      if (!footerData.legalLinks) {
	        return;
	      }

	      for (var i = 0; i < footerData.legalLinks.length; i++) {
	        var legalLink = footerData.legalLinks[i];
	        legalLink.classNames = [];

	        if (legalLink.flags) {
	          var candidates = common.map(legalLink.flags, function (k, v) {
	            if (v) {
	              return k;
	            }
	          });
	          legalLink.classNames = common.filter(candidates, function (v) {
	            return !!v;
	          });
	        }
	      }
	    },
	    processCopyrightYear: function processCopyrightYear() {
	      if (!footerData.copyright) {
	        return;
	      }

	      var currentYear = new Date().getFullYear();
	      var riotCopyright = footerData.copyright.riotCopyright;

	      if (riotCopyright) {
	        footerData.copyright.riotCopyright = riotCopyright.replace('%year%', currentYear);
	      }

	      var leagueCopyright = footerData.copyright.leagueCopyright;

	      if (leagueCopyright) {
	        footerData.copyright.leagueCopyright = leagueCopyright.replace('%year%', currentYear);
	      }

	      if (footerData.copyright.messages) {
	        for (var m = 0; m < footerData.copyright.messages.length; m++) {
	          var message = footerData.copyright.messages[m];
	          footerData.copyright.messages[m] = message.replace('%year%', currentYear);
	        }
	      }
	    },
	    processCopyrightMessageLinks: function processCopyrightMessageLinks() {
	      /*jshint maxcomplexity:9*/
	      if (!footerData.copyright.messageLinks || !footerData.copyright.messages) {
	        return;
	      }

	      for (var ml = 0; ml < footerData.copyright.messageLinks.length; ml++) {
	        var messageLink = footerData.copyright.messageLinks[ml];

	        if (!messageLink.token || !messageLink.text || !messageLink.url) {
	          continue;
	        }

	        var tokenString = '{' + messageLink.token + '}';

	        for (var m = 0; m < footerData.copyright.messages.length; m++) {
	          var message = footerData.copyright.messages[m];

	          if (message.indexOf(tokenString) === -1) {
	            continue;
	          }

	          var linkHTML = '<a href="' + messageLink.url + '" target="_blank">' + messageLink.text + '</a>';
	          footerData.copyright.messages[m] = message.replace(tokenString, linkHTML);
	        }
	      }
	    },
	    renderFooter: function renderFooter() {
	      if (!isDataProcessed) {
	        return null;
	      } // Add in the font and locale styles if we are being loaded as a standalone


	      common.appendStyles(customFonts);
	      var footerStyles = [stylesFooterContainer, stylesFooterLinks, stylesRiotLogo, stylesRiotFooterLocale, stylesSocialLinks, stylesLegalLinks, stylesCopyright, stylesGameRatingLogos];
	      var footerConfig = this.config;
	      common.appendStyles(footerStyles.join(' '));
	      this.determineContainerElement().then(function (footerElement) {
	        var renderContext = footerData;
	        var footerTopMargin = footerConfig.container && footerConfig.container.marginTop ? parseInt(footerConfig.container.marginTop, 10) : 0;
	        var locale = null;

	        if (window.RiotBar && window.RiotBar.plugins && window.RiotBar.plugins.locale) {
	          locale = window.RiotBar.plugins.locale;
	        } else {
	          var localeConfig = {
	            locales: i18n_1.getDefaultLocales(),
	            landingUrlPattern: i18n_1.getDefaultLandingURLPattern()
	          };
	          locale = {
	            config: localeConfig,
	            localeUtils: localeUtils(localeConfig)
	          };
	        }

	        renderFooterComponent(footerElement, {
	          marginTop: footerTopMargin,
	          disableFooterBackground: footerConfig.container && footerConfig.container.disableFooterBackground,
	          footerLinksEnabled: true,
	          //  @Jason - remove need for this property - should show if we have links
	          renderContext: renderContext,
	          locale: locale,
	          socialLinksEnabled: true //  @Jason - remove need for this property - should show if we have links

	        });
	      }).catch(function (err) {
	        // No element to render into - silently fail out just with a notice that we are not rendering the bar
	        console && console.log("No footer element determined. Not rendering footer");
	      });
	    },

	    /**
	     * Initialization function to be called by the footer plugin.
	     *
	     * @param  {Object} config          Configuration for this plugin.
	     * @param  {DOMElement} baseElement Base DOM element for the bar.
	     */
	    init: function init(configOverrides, renderRegions, global) {
	      this.config = common.deepOverride(configDefaults, configOverrides || {}); // the enabled config is only used by riotbar to determine
	      // if we should load the deferred script.
	      // if the code gets here it should be enabled,
	      // to simplify usage of the standalone footer script.

	      this.config.enabled = true;

	      if (!riotbarData.footer) {
	        return;
	      } // Because footerLinks and socialLinks can conflict with the desired set of passed links
	      // compared to what is sent from loaded regional build data (object vs array)
	      // we will treat those properties special


	      var linkProps = ['footerLinks', 'socialLinks'];

	      for (var _i = 0, _linkProps = linkProps; _i < _linkProps.length; _i++) {
	        var prop = _linkProps[_i];

	        if (configOverrides[prop] && configOverrides[prop].enabled && configOverrides[prop].links) {
	          riotbarData.footer[prop] = configOverrides[prop].links;
	          delete configOverrides[prop];
	        } else if (configOverrides[prop] && configOverrides[prop].enabled) {
	          delete configOverrides[prop];
	        }
	      }

	      footerData = common.deepOverride(riotbarData.footer, configOverrides);
	      var defaultGlobal = globalConfig.getDefaultGlobalConfig();

	      if (!global) {
	        try {
	          global = RiotBar.config.global || defaultGlobal;
	        } catch (err) {
	          global = defaultGlobal;
	        }
	      }

	      this.processData();

	      if (!this.config.disableRender) {
	        this.renderFooter(global);
	      }
	    }
	  };

	  if (window.RiotBar !== undefined) {
	    window.RiotBar.plugins.footerContent = footerContent;
	  } else {
	    window.RiotbarFooter = footerContent;
	  }
	});
	var deferred_1 = deferred.config;
	var deferred_2 = deferred.styles;
	var deferred_3 = deferred.footerContainerElement;
	var deferred_4 = deferred.getFooterData;
	var deferred_5 = deferred.getRiotLogoData;
	var deferred_6 = deferred.getCopyrightData;
	var deferred_7 = deferred.getFooterLinksData;
