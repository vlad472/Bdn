	var deferred = createCommonjsModule(function (module) {
	  'use strict';

	  var CookiePolicyV2 = module.exports = {
	    config: {},
	    data: {},

	    /**
	     * @type {Object}
	     *   The default settings for this plugin. Any settings that could be
	     *   overriden by the users need to be here.
